{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./",
      "types": ["node"]
    },
    "include": ["seed.ts", "seed-config.ts"],
    "ts-node": {
      "esm": true,
      "experimentalSpecifierResolution": "node"
    }
  }